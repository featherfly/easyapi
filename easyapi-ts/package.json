{
  "name": "easyapi-ts",
  "version": "0.2.2",
  "main": "index.js",
  "types": "dist/request.d.ts",
  "typings": "dist/request.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/featherfly/easyapi"
  },
  "exports": {
    ".": {
      "import": {
        "node": "./index.mjs",
        "default": "./dist/request.js"
      },
      "require": "./index.js",
      "types": "./dist/request.d.ts"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "yarn build:uni && yarn build:axios",
    "build:uni": "gulp clean && gulp copyRelease && gulp compile && gulp copyUni && gulp uglify && gulp cleanCompile",
    "build:axios": "gulp clean && gulp copyRelease && gulp compile && gulp buildAxios && gulp uglify && gulp cleanCompile",
    "build:uni2": "gulp clean && gulp buildUni && gulp cleanCompile",
    "build:axios2": "gulp clean && gulp buildAxios && gulp cleanCompile",
    "pub": "npm run pub:uni && npm run pub:axios",
    "pub:uni": "yarn build:uni && npm publish ./release",
    "pub:axios": "yarn build:axios && npm publish ./release"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "^17.0.19",
    "@types/uni-app": "^1.4.4",
    "axios": "^0.24.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {}
}
